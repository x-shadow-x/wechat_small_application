page, .content {
    height: 100%;
}

.left_list_btn {
    display: inline-block;
    width: 20px;
    height: 20px;
}

.list_item {
    padding: 10px 10px;
    border-bottom: 1px solid #ccc;
}
.store_icon, .store_info_box, .handle_box {
    display: inline-block;
    vertical-align: middle;
}
.store_icon {
     width: 10%;
    padding-bottom: 20%;
    background: url("../imgs/store_icon/timthumb.jpg") left center no-repeat;
    background-size: 100% auto;
 }

.store_info_box {
    width: 62%;
    padding-left: 5%;
    padding-right: 1%;
}

.blod_text {
    font-weight: bold;
}

.store_name {
    font-size: 16px;
    font-weight: bold;
}

.store_address,
.food_type {
    white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    width: 96%;
}

.store_address {
    margin-bottom: 10px;
}

.handle_box {
    width: 22%;
}

.menu_link {
    background: #DD403B;
    color: #fff;
    border-radius: 3px;
    padding: 6px 5px;
    text-align: center;
}
.start_box {
    margin-top: 10px;
}
.start {
    display: inline-block;
    width: 18%;
    height: 0;
    padding-top: 18%;
    margin: 0 1%;
    background: url('../imgs/empty_start.png') center no-repeat;
    background-size: 100% auto;
}
.start.active {
    background-image: url('../imgs/active_start.png');
}

.bottom_line {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 0;
    border-top: 1px solid #ccc;
}